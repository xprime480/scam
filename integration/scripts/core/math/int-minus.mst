Test operator - on integers

;[skip] uncomment to skip this test

[name]
Integer Minus

[input]

(define check
  (lambda (val)
    (list val (integer? val) (exact? val))))

(check (-))
(check (- 2))
(check (- 7 2))
(check (- 2 2 -1 -3))
(check (- 2 2 -1 -3.0))

[result]
[keep 5]
(0 #t #t)
(-2 #t #t)
(5 #t #t)
(4 #t #t)
(4 #t #f)
