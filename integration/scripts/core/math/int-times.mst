Test operator * on integers

;[skip] uncomment to skip this test

[name]
Integer Times

[input]

(define check
  (lambda (val)
    (list val (integer? val) (exact? val))))

(check (*))
(check (* 2))
(check (* 2 3))
(check (* 2 2 -1 -3 4))
(check (* 2 2 -1 0 4))
(check (* 2 2 -1 -3 4.0))

[result]
[keep 6]
(1 #t #t)
(2 #t #t)
(6 #t #t)
(48 #t #t)
(0 #t #t)
(48 #t #f)
