Test operator - on rationals

;[skip] uncomment to skip this test

[name]
Rational Minus

[input]

(define check
  (lambda (val)
    (list val
          (rational? val)
          (integer? val)
          (exact? val))))

(check (- 5/2))
(check (- 2/3 1/3))
(check (- 4/3 1/3))
(check (- #i1/3 1/3 1/3))

[result]
[keep 4]
(-5/2 #t #f #t)
(1/3 #t #f #t)
(1 #t #t #t)
(-1/3 #t #f #f)
