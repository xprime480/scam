Test operator * on complex numbers

;[skip] uncomment to skip this test

[name]
Complex Times

[input]

(define check
  (lambda (val)
    (list val
          (complex? val)
          (real? val)
          (rational? val)
          (integer? val)
          (exact? val))))

(check (* 1+i))
(check (* 1+i 2-3i))
(check (* 1+i 1-i))

[result]
[keep 3]
(1+i #t #f #f #f #t)
(5-i #t #f #f #f #t)
(2 #t #t #t #t #t)